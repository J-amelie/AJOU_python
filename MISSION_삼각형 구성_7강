ft = int(input('첫 번째 변의 길이를 입력하세요 : '))
st = int(input('두 번째 변의 길이를 입력하세요 : '))
tt = int(input('세 번째 변의 길이를 입력하세요 : '))
lt = int


lt=max(ft,st,tt)

if(lt==ft) :
    if(lt >= (st+tt)) :
        print('삼각형을 구성할 수 없습니다.')
    else:
        print('삼각형입니다.')
elif(lt==st) :
    if(lt >= (ft+tt)):
        print('삼각형을 구성할 수 없습니다.')
    else:
        print('삼각형입니다.')
else:
    if(lt >= (ft+st)):
        print('삼각형을 구성할 수 없습니다.')
    else:
        print('삼각형입니다.')